{
  "name": "travel_hive",
  "version": "0.1.0",
  "private": true,
  "dependencies": {    
    "bcrypt-nodejs": "0.0.3",    
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "mysql": "^2.15.0",
    "mysql2": "^1.5.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",    
    "sequelize": "^4.28.6",
    "sequelize-cli": "^3.2.0",
    "winston": "^2.4.0"
  },
  "scripts": {
    "start": "node ./dist/server.js",
    "server": "cross-env NODE_ENV=development nodemon --exec babel-node ./server/server.js",
    "server:build": "babel server -s -D -d dist",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "db:create": "docker run --name travel_hive_mysql -p 3306:3306 -e MYSQL_ROOT_PASSWORD=\"root\" -dit mysql",
    "db:migrate": "./node_modules/.bin/sequelize db:migrate",
    "db:destroy": "docker rm travel_hive_mysql",
    "db:start": "docker start travel_hive_mysql",
    "db:stop": "docker stop travel_hive_mysql"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.1.3",
    "eslint": "4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.4.0",
    "nodemon": "^1.12.5"
  }
}
